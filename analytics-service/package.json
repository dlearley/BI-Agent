{
  "name": "bi-agent-analytics",
  "version": "1.0.0",
  "description": "BI-Agent Analytics Service with KPIs, RBAC, and HIPAA compliance",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:ui": "playwright test --ui",
    "migrate": "node dist/database/migrate.js",
    "migrate:rollback": "node dist/database/migrate.js rollback",
    "migrate:status": "node dist/database/migrate.js status",
    "seed": "npm run build && npm run migrate && node dist/scripts/seed.js",
    "analytics:run": "cd ../../dbt && dbt run",
    "analytics:test": "cd ../../dbt && dbt test",
    "analytics:docs": "cd ../../dbt && dbt docs generate && dbt docs serve",
    "analytics:refresh": "node dist/scripts/refresh-analytics.js",
    "analytics:refresh:all": "node dist/scripts/refresh-analytics.js refresh",
    "analytics:refresh:pipeline": "node dist/scripts/refresh-analytics.js refresh pipeline",
    "analytics:refresh:compliance": "node dist/scripts/refresh-analytics.js refresh compliance",
    "analytics:refresh:revenue": "node dist/scripts/refresh-analytics.js refresh revenue",
    "analytics:refresh:outreach": "node dist/scripts/refresh-analytics.js refresh outreach",
    "setup": "chmod +x ../../jobs/setup.sh && ../../jobs/setup.sh",
    "setup:full": "../../jobs/setup.sh --with-sample-data --with-dbt --with-tests",
    "lint": "eslint . --ext .ts && tsc --noEmit",
    "lint:fix": "eslint . --ext .ts --fix",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f"
  },
  "dependencies": {
    "@types/uuid": "^10.0.0",
    "bcryptjs": "^2.4.3",
    "bullmq": "^4.15.4",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "csv-parse": "^5.5.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.11.3",
    "redis": "^4.6.10",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "zod": "^3.22.4",
    "@opentelemetry/sdk-node": "^0.45.1",
    "@opentelemetry/api": "^1.7.0",
    "@opentelemetry/auto-instrumentations-node": "^0.40.3",
    "@opentelemetry/exporter-trace-otlp-http": "^0.45.1",
    "@opentelemetry/exporter-metrics-otlp-http": "^0.45.1",
    "@opentelemetry/sdk-metrics": "^1.19.0",
    "@opentelemetry/instrumentation-express": "^0.34.1",
    "@opentelemetry/instrumentation-http": "^0.45.1",
    "@opentelemetry/instrumentation-pg": "^0.45.0",
    "@opentelemetry/instrumentation-redis-4": "^0.43.0",
    "@opentelemetry/instrumentation-ioredis": "^0.45.0",
    "@opentelemetry/resources": "^1.19.0",
    "@opentelemetry/semantic-conventions": "^1.19.0",
    "prom-client": "^15.1.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.40.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.7.5",
    "@types/node": "^20.8.10",
    "@types/uuid": "^9.0.7",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.7",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.8.10",
    "@types/pg": "^8.10.7",
    "@types/supertest": "^2.0.16",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "eslint": "^8.53.0",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "tsx": "^4.1.4",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
