{
  "annotations": {
    "list": []
  },
  "editable": true,
  "gnetId": null,
  "graphTooltip": 0,
  "panels": [
    {
      "datasource": "Prometheus",
      "fieldConfig": {
        "defaults": {
          "unit": "ops"
        }
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 0
      },
      "id": 1,
      "options": {
        "legend": {
          "calcs": ["mean", "max"],
          "displayMode": "table",
          "placement": "right"
        }
      },
      "targets": [
        {
          "expr": "sum(rate(queue_jobs_total{status=\"completed\"}[5m])) by (queue, job_type)",
          "legendFormat": "{{queue}} - {{job_type}}",
          "refId": "A"
        }
      ],
      "title": "Job Throughput",
      "type": "timeseries"
    },
    {
      "datasource": "Prometheus",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 0
      },
      "id": 2,
      "options": {
        "legend": {
          "calcs": ["mean", "max"],
          "displayMode": "table",
          "placement": "right"
        }
      },
      "targets": [
        {
          "expr": "sum(rate(queue_jobs_total{status=\"failed\"}[5m])) by (queue, job_type)",
          "legendFormat": "{{queue}} - {{job_type}}",
          "refId": "A"
        }
      ],
      "title": "Failed Job Rate",
      "type": "timeseries"
    },
    {
      "datasource": "Prometheus",
      "fieldConfig": {
        "defaults": {
          "unit": "s"
        }
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 8
      },
      "id": 3,
      "options": {
        "legend": {
          "calcs": ["mean", "max"],
          "displayMode": "table",
          "placement": "right"
        }
      },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(queue_job_duration_seconds_bucket[5m])) by (queue, job_type, le))",
          "legendFormat": "P95 {{queue}} - {{job_type}}",
          "refId": "A"
        }
      ],
      "title": "Job Duration",
      "type": "timeseries"
    },
    {
      "datasource": "Prometheus",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 8
      },
      "id": 4,
      "targets": [
        {
          "expr": "queue_size{state=\"waiting\"}",
          "legendFormat": "Waiting - {{queue}}",
          "refId": "A"
        },
        {
          "expr": "queue_size{state=\"active\"}",
          "legendFormat": "Active - {{queue}}",
          "refId": "B"
        }
      ],
      "title": "Queue Size",
      "type": "timeseries"
    },
    {
      "datasource": "Prometheus",
      "fieldConfig": {
        "defaults": {
          "unit": "short"
        }
      },
      "gridPos": {
        "h": 4,
        "w": 6,
        "x": 0,
        "y": 16
      },
      "id": 5,
      "options": {
        "reduceOptions": {
          "calcs": ["lastNotNull"],
          "values": false
        },
        "showThresholdLabels": false,
        "showThresholdMarkers": true
      },
      "targets": [
        {
          "expr": "sum(queue_size{state=\"failed\"})",
          "refId": "A"
        }
      ],
      "title": "Failed Jobs (Current)",
      "type": "gauge"
    },
    {
      "datasource": "Prometheus",
      "gridPos": {
        "h": 4,
        "w": 6,
        "x": 6,
        "y": 16
      },
      "id": 6,
      "targets": [
        {
          "expr": "sum(queue_size{state=\"delayed\"})",
          "refId": "A"
        }
      ],
      "title": "Delayed Jobs",
      "type": "gauge"
    }
